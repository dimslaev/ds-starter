{
  "name": "DS",
  "version": "1.0.0",
  "description": "Starter slim version.",
  "main": "index.js",
  "scripts": {
    "js:build": "babel assets/js/main.js --out-file assets/js/app.js",
    "js:watch": "babel assets/js/main.js --watch --out-file assets/js/app.js",
    "css:build": "node-sass --include-path scss assets/scss/main.scss assets/css/main.css",
    "css:watch": "nodemon -e scss -x \"npm run css:build\"",
    "watch": "concurrently \"npm:js:watch\" \"npm:css:watch\"",
    "lint": "eslint ."
  },
  "author": "Dimslaev",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-watch": "^2.0.7",
    "concurrently": "^4.1.0",
    "eslint": "^5.9.0",
    "node-sass": "^4.10.0",
    "nodemon": "^1.18.6"
  }
}
